{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Verifier": {
    "Audience": "urn:example:verifier",
    "VerifyMethod": "POST",
    "VerifyPath": "/verify",
    "NonceTtlSeconds": 120,
    "ClockSkewSeconds": 120
  }
}
